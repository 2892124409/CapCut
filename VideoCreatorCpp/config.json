{
  "project_name": "Demo Video Project",
  "output": {
    "output_path": "output/video.mp4",
    "width": 1920,
    "height": 1080,
    "frame_rate": 30,
    "video_bitrate": 4000000,
    "audio_bitrate": 128000,
    "video_codec": "libx264",
    "audio_codec": "aac"
  },
  "scenes": [
    {
      "name": "Opening Scene",
      "duration": 5.0,
      "resources": [
        {
          "path": "assets/image1.jpg",
          "type": "IMAGE",
          "start_time": 0.0,
          "duration": 5.0,
          "volume": 1.0
        },
        {
          "path": "assets/audio1.mp3",
          "type": "AUDIO",
          "start_time": 0.0,
          "duration": 5.0,
          "volume": 0.8
        }
      ],
      "effects": [
        {
          "type": "FADE_IN",
          "start_time": 0.0,
          "duration": 1.0,
          "params": ""
        },
        {
          "type": "FADE_OUT",
          "start_time": 4.0,
          "duration": 1.0,
          "params": ""
        }
      ]
    },
    {
      "name": "Main Scene",
      "duration": 10.0,
      "resources": [
        {
          "path": "assets/video1.mp4",
          "type": "VIDEO",
          "start_time": 0.0,
          "duration": 10.0,
          "volume": 1.0
        },
        {
          "path": "assets/audio2.mp3",
          "type": "AUDIO",
          "start_time": 0.0,
          "duration": 10.0,
          "volume": 0.9
        }
      ],
      "effects": [
        {
          "type": "ZOOM",
          "start_time": 2.0,
          "duration": 3.0,
          "params": "zoom=1.2"
        }
      ]
    },
    {
      "name": "Ending Scene",
      "duration": 3.0,
      "resources": [
        {
          "path": "assets/image2.jpg",
          "type": "IMAGE",
          "start_time": 0.0,
          "duration": 3.0,
          "volume": 1.0
        }
      ],
      "effects": [
        {
          "type": "FADE_OUT",
          "start_time": 2.0,
          "duration": 1.0,
          "params": ""
        }
      ]
    }
  ]
}
